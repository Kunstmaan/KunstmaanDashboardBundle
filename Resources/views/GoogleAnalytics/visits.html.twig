{# Twig vars are commented: they are loaded with AJAX, they are still here as a placeholder #}

<div class="db-block db-col__inner">
    <div id="dashboard-chart--visits" class="dashboard-chart dashboard-chart--donut"></div>


     <ul class="db-block__list" id="data_visits">
        <li class="db-block__list__item">
            {# new visitors #}
            <p class="db-block__text db-block__text--left">
                {{ 'analytics.visitors.new' | trans }}
            </p>
            <p class="db-block__stats db-block__stats--right" id="data_new_visits">

                <span style="color:rgba(41, 200, 150, 1)">
                    <span class="data_percentage">{# {{ overview.getNewVisitsPercentage }} #}</span>
                    <span class="data_visits">{# {{ overview.newVisits }} #}</span>
                </span>

            </p>
        </li>
        <li class="db-block__list__item">
            {# returning visitors #}
            <p class="db-block__text db-block__text--left">
                {{ 'analytics.visitors.returning' | trans }}
            </p>
            <p class="db-block__stats db-block__stats--right" id="data_returning_visits">

                <span style="color:rgba(41, 151, 206, 1)">
                    <span class="data_percentage">{# {{ overview.getReturningVisitsPercentage }} #}</span>
                    <span class="data_visits">{# {{ overview.returningVisits }} #}</span>
                </span>

            </p>
        </li>
    </ul>
</div>
